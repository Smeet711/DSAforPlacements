class Solution {
    public int triangularSum(int[] nums) {
        
       while(nums.length > 1){
           
           int[] t = new int[nums.length-1];
           for(int i=0;i<t.length;i++){
               t[i] += (nums[i] + nums[i+1]) % 10;
           }
           nums = t;
       }
        return nums[0];
        
    }
}
